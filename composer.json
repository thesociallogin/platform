{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-google-fonts-plugin": "^3.2",
        "inertiajs/inertia-laravel": "^1.3",
        "laravel/framework": "^11.31",
        "laravel/passport": "^12.0",
        "laravel/tinker": "^2.9",
        "league/oauth2-client": "^2.7",
        "league/oauth2-google": "^4.0",
        "spatie/laravel-data": "^4.11",
        "spatie/laravel-google-fonts": "^1.4",
        "tailflow/laravel-orion": "^2.23"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.2",
        "fakerphp/faker": "^1.23",
        "itsgoingd/clockwork": "^5.3",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "analyze": "vendor/bin/phpstan analyse --memory-limit=2G",
        "cs-fix": "vendor/bin/pint",
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "docs:models": [
            "@php artisan ide-helper:models -nRW",
            "@cs-fix app/Models"
        ],
        "ide": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -nRW",
            "@cs-fix app/Models"
        ],
        "lint": "@cs-fix",
        "reset": [
            "@composer install --no-interaction --no-progress",
            "npm ci --no-audit --no-fund",
            "npm run build",
            "@php artisan migrate:fresh --seed",
            "@php artisan horizon:purge",
            "@php artisan optimize:clear",
            "@ide"
        ],
        "tc": "@test-coverage",
        "test": "@php artisan test --no-coverage --parallel",
        "test-coverage": "@php artisan test --coverage --parallel",
        "test-filter": "@php artisan test --no-coverage --filter",
        "test-lint": "@cs-fix --test",
        "test-suite": [
            "@analyze",
            "@test"
        ],
        "test-types": "@analyze",
        "test:filter": "@test-filter",
        "test:lint": "@test-lint",
        "test:suite": "@test-suite",
        "test:types": "@analyze",
        "tf": "@test-filter",
        "tl": "@test-lint",
        "ts": "@test-suite",
        "tt": "@test-types"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
